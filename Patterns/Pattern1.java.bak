public class Pattern1
{
	public static void main(String x[])
	{
		for(int i = 1; i <= 9; i++)
		{
			for(int j = 1; j <= 9; j++)
			{
				if((i <= j && i <= 5) && ( j < 11 - i && i <= 5) || (i> 4 && j >=10  - i) && (i >=6 && j <= i))
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}