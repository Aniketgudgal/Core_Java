public class Pattern9
{
	public static void main(String x[])
	{
		int n = 10; // this is the size of diamond in dynamic. the diamond only accept the even number value 
		for(int i = 1; i <= n; i++)
		{
			int k = 1;
			boolean flag = true;
			for(int j=1; j <= n - 1; j++)
			{
				if(((i <= 5 && j >= 6 - i) && (i <=5 && j <= 4 + i)) 
					&&
				(i >= 5 && j >= 4 + i) && ( i >= 5 &&  j <= 14- i)&& flag)
				{
					System.out.print(k++);
					flag = false;
				}
				else
				{
					System.out.print(" ");
					flag = true;
				}
			}
			System.out.println();
		}
	}
}