/*
Storing the Object in vector
*/
import java.util.*;
class Employee
{
	private int id;
	private String name;
	private int salary;
	Employee(int id, String name, int salary)
	{
		this.id = id;
		this.name = name;
		this.salary = salary;
	}
	public int getId()
	{
		return id;
	}
	public String getName()
	{
		return name;
	}
	public int getSalary()
	{
		return salary;
	}
}
public class ObjStore
{
	public static void main(String x[])
	{
		Vector vt = new Vector();
		Employee e1 = new Employee(1,"Aniket",4000);
		Employee e2 = new Employee(2,"yd",5000);
		Employee e3 = new Employee(3,"gb",6000);
		vt.add(e1);
		vt.add(e2);
		vt.add(e3);
		Iterator i = vt.iterator();
		while(i.hasNext())
		{
			Object obj = i.next();
			Employee e = (Employee)obj;
			System.out.println(e.getId()+"\t"+e.getName()+"\t"+e.getSalary());
		}
	}
}