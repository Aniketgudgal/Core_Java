		// method 2
		int i = 0;
		int temp = n;
		while(i < 10)
		{
			int count = 0;
			while(temp != 0)
			{
				int rem = temp % 10;
				if(rem == i)
					count++;
				temp /= 10;
			}
			if(count > 0)
			{
				System.out.println("the frequence of "+i+" is: "+count);
			}
			i++;
			temp = n;
		}
