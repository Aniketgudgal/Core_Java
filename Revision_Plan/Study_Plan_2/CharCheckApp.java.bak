/*
Q2. Write program to create class name as CheckChar with two functions
        void setChar(char ch): this function accept single character as input
      boolean checkChar(): this function can check character is alphabet or digit of special symbol 
	  if character is alphabet or digit return true if character is digit then return false.
*/
import java.util.Scanner;
class CheckChar
{
	char ch;
	void setChar(char ch)
	{
		this.ch = ch;
	}
	boolean checkChar()
	{
		if(ch >= 65 && ch <= 90 || ch >= 96 && ch <= 122 || ch >= 48 && ch <= 57)
		{
			System.out.println((int)ch);
			return true;
		}
		else
		{
			return false;
		}
	}
}
public class CharCheckApp
{
	public static void main(String x[])
	{
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the character: ");
		char ch = sc.next().charAt(0);
		
		CheckChar c = new CheckChar();
		c.setChar(ch);
		
		if(c.checkChar())
		{
			System.out.println("The input character is alphabet or digit");
		}
		else
		{
			System.out.println("The input character is special symbol");
		}
	}
}