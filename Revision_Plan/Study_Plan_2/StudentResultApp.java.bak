/*
Q.8Create a base class Student with a constructor Student(String name, int marks) to store values.
 Create a child class Result that extends Student and has function getGrade() which returns:
"A Grade" if marks ≥ 75
"B Grade" if marks ≥ 50
"Fail" otherwise.
*/
import java.util.Scanner;
class Student
{
	String name;
	int marks;
	Student(String name, int marks)
	{
		this.name = name;
		this.marks = marks;
	}
}
class Result extends Student
{
	Result(String name, int marks)
	{
		super(name, marks);
	}
	void getGrade()
	{
		if(marks >= 75)
		{
			System.out.println("A Grade");
		}
		else if(marks >= 50)
		{
			System.out.println("B Grade");
		}
		else
		{
			System.out.println("Fail");
		}
	}
}
public class StudentResultApp
{
	public static void main(String x[])
	{
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the name of student: ");
		String name = sc.nextInt();
		System.out.print("Enter the marks of student: ");
		int marks = sc.nextInt();
		
		Result r = new Result(name,marks);
		r.getGrade();
	}
}